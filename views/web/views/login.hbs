                                    $(".loginpmsg").html("Something went wrong.");
                                    $(".errors").css("display", "block");
                                    setTimeout(function() {
                                        $(".loginpmsg").html('');
                                        $(".errors").css("display", "none");
                                    }, 3000);
                                }
                            }
