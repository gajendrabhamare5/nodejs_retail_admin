                function sendOtp() {
                    $(".loginbtn").html('Loading...');
                    $(".loginbtn").css('pointer-events', 'none');
                    /* var code = $("#txtPhone").intlTelInput("getSelectedCountryData").dialCode; */
                    var code = '91';
                    var phoneNumber = $('#txtPhone').val();
                    if (phoneNumber == "") {
                        $(".loginbtn").html('Send OTP');
                        $(".loginbtn").css('pointer-events', 'auto');
                        $(".loginpmsg").css("color", "red");
                        $(".loginpmsg").html("Enter Mobile No.");
                        $(".errors").css("display", "block");
                        setTimeout(function() {
                            $(".loginpmsg").html('');
                            $(".errors").css("display", "none");
                        }, 3000);
                    } else if (phoneNumber.length != 10) {
                        $(".loginbtn").html('Send OTP');
                        $(".loginbtn").css('pointer-events', 'auto');
                        $(".loginpmsg").css("color", "red");
                        $(".loginpmsg").html("Mobile No. should be 10 digit.");
                        $(".errors").css("display", "block");
                        setTimeout(function() {
                            $(".loginpmsg").html('');
                            $(".errors").css("display", "none");
                        }, 3000);
                    } else {
                        var Type = "send_otp";
                        $.ajax({
                            type: "POST",
                            dataType: "JSON",
                            url: baseurl + "ajaxfiles/account.php",
                            data: {
                                Type: Type,
                                phoneNumber: phoneNumber,
                                code: code
                            },
                            success: function(data) {
                                //alert();
                                if (data.status == "error") {
                                    $(".loginbtn").html('Send OTP');
                                    $(".loginbtn").css('pointer-events', 'auto');
                                    $("#loginbtn").css("pointer-events", "visible");
                                    $(".loginpmsg").css("color", "red");
                                    $(".loginpmsg").html(data.message);
                                    $(".errors").css("display", "block");
                                    setTimeout(function() {
                                        $(".loginpmsg").html('');
                                        $(".errors").css("display", "none");
                                    }, 3000);

                                } else if (data.status == "ok") {
                                    $("#loginbtn").css("pointer-events", "visible");
                                    $(".loginbtn").css('pointer-events', 'auto');
                                    $(".loginpmsg").css("color", "green");
                                    $(".loginpmsg").html(data.message);
                                    $(".errors").css("display", "block");
                                    $(".otpSec").show();
                                    $(".loginbtn").attr('onclick', 'verify_otp()');
                                    $(".loginbtn").html('Submit');
                                    counter();
                                    /* setTimeout(function() {
                                        $(".loginpmsg").html('');
                                        $(".errors").css("display", "none");
                                    }, 3000); */

                                } else {
                                    $(".loginbtn").html('Send OTP');
