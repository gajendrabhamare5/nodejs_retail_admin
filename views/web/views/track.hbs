<script>
    $(document).ready(function() {
        $("#testimonial-slider").owlCarousel({
            items: 1,
            itemsDesktop: [1000, 2],
            itemsDesktopSmall: [980, 1],
            itemsTablet: [768, 1],
            pagination: true,
            navigation: false,
            navigationText: ["", ""],
            autoPlay: true
        });
    });

    function submit() {

        var tracknumbers = $("#tracknumbers").val();
        var email = $("#email").val();

        $.ajax({
            type: "POST",
            data: {
                tracknumbers: tracknumbers,
                email: email
            },
            dataType: "json",
            url: "<?php echo $base_url; ?>ajaxfiles/get_tracking.php",
            success: function(response) {
                console.log(response.status);
                console.log(response.message);
                if (response.status == "ok") {
                    $("#show_data").html("");
                    var show_data = `<table id="customers">
														<tr>
															<th>tracking_code</th>
															<td>${response.message.tracking_code}</td>

														</tr>
														<tr>

															<th>carrier_name</th>
															<td>${response.message.carrier_name}</td>

														</tr>
														<tr>

															<th>courier_website</th>
															<td>${response.message.courier_website}</td>

														</tr>
														<tr>


															<th>date</th>
															<td>${response.message.pickup_date}</td>
														</tr>

													</table>`
                    $("#show_data").html(show_data);

                } else {
                    var show_data = `<span>NO DATA AVAILABLE</span>`;
                    $("#show_data").html("");
                    $("#show_data").html(show_data);
                }
            }
        });

    }
</script>


