    <script>
        function showHidePassword() {
            var passtype = $(".passInputElement").attr('type');
            var passval = $(".passInputElement").val();
            var oldInput = $(".passInputElement");
            if (passtype == "password") {
                var newInput = $('<input>').attr({
                    type: 'text',
                    id: 'loginInputPassword1',
                    name: 'customer[password]',
                    class: 'passInputElement input-full',
                    value: passval
                });
                $(".showpasshide").removeClass('fa-eye-slash').addClass('fa-eye');
            } else {
                var newInput = $('<input>').attr({
                    type: 'password',
                    id: 'loginInputPassword1',
                    name: 'customer[password]',
                    class: 'passInputElement input-full',
                    value: passval
                });
                $(".showpasshide").removeClass('fa-eye').addClass('fa-eye-slash');
            }
            oldInput.replaceWith(newInput);
        }
    </script>
