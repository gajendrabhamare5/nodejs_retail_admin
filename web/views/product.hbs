    <script>
        var pro_id = '<?php echo $product_id; ?>';
        var user_id = '<?php echo $user_id_cookie; ?>';
        var cookieName = 'recently_viewed_products_' + user_id;
        if (document.cookie.indexOf(cookieName) >= 0) {
            var myCookieValue = getCookie(cookieName);
            var convertInArray = myCookieValue.split(",");
            if (convertInArray.includes(pro_id) == false) {
                convertInArray.push(pro_id);
            }
            if (convertInArray.length > 10) {
                convertInArray.splice(0, 1);
            }
            var newValue = convertInArray.join(",");
            document.cookie = cookieName + "=" + newValue;
        } else {
            var date = new Date();
            date.setFullYear(date.getFullYear() + 10); // Add 10 years to the current date

            document.cookie = cookieName + "=" + pro_id + "; expires=" + date.toUTCString() + "; path=/";
        }

        function delete_cookie(cookieName) {
            document.cookie = cookieName + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
        }

        function getCookie(cookieName) {
            var cookies = document.cookie.split(";");
            // Iterate over the cookies
            for (var i = 0; i < cookies.length; i++) {
                var cookie = cookies[i].trim();
                // Check if the cookie starts with the provided name
                if (cookie.indexOf(cookieName + "=") === 0) {
                    // Extract and return the cookie value
                    return cookie.substring(cookieName.length + 1, cookie.length);
                }
            }
            // Cookie not found
            return null;
        }
    </script>
